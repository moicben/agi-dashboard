# Configuration Incus
INCUS_SERVER=XXXX.XXX.XXX
INCUS_PORT=9443
IP_PREFIX=10.225.44.XXX

# API Incus
INCUS_API_URL=https://mydomain.com/instances
INCUS_API_KEY=your-api-key-here

# Supabase
# Utilisé par les routes API Next.js (dashboard/pages/api/*)
SUPABASE_URL=https://xxxxxxxxxxxxxxxxxxxxxxxx.supabase.co
# Clé publishable (anon) — déjà utilisée par certaines routes (/api/analytics, etc.)
SUPABASE_ANON_KEY=your-anon-key-here
# Optionnel mais recommandé pour Storage/listing/signed URLs
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Port du serveur (optionnel)
PORT=3000
